
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read/write access for development purposes.
    // IMPORTANT: These rules are not secure and should be updated
    // for production. They expire on January 1, 2025.
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2025, 1, 1);
    }
  }
}
